{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug C Project",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/output/philosophers",
      "args": ["8", "800", "100", "100", "2"],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}
